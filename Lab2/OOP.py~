class Animal:
	def __init__(self, name, age):
		self.name=name
		self.age=age
	
	def sleep(self):
		print self.name + "is sleeping"

	def eat(self):
		print self.name + "is eating"

class Kangaroo(Animal):
	
	def __init__(self, joeyName)
		Animal__init__(name, age)
		self.joeyName = joeyName
	
	def jumping (self, jumps):
		print self.name + "jumped" + jumps + "times"

	def child(self):
		print self.name + "'s joey is named" + self.joeyName

class Dog(Animal):

	def __init__(self, breed, color):
		Animal__init__(name, age)
		self.breed=breed
		self.color=color

	def bark (self):
		print self.name + "is barking"
	
